{
   "manifest_version": 3, 

   "name": "Ad Blocker New",
   "description": "The new Ad blocker",
   "version": "1.0.0",
    "icons":{"128": "icon_128.png"},
    "action": {
       "default_icon": "icon.png",
       "default_popup": "popup.html"
    },
    "content_scripts": [{
       "matches": ["<all_urls>"],
       "js": ["contentscript.js"]
     }],
      "permissions": [ "tabs", "storage"]
}